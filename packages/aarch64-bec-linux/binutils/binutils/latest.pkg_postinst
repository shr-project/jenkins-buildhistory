#!/bin/sh
set -e
	update-alternatives --install /usr/bin/addr2line addr2line /usr/bin/aarch64-bec-linux-addr2line 100
	update-alternatives --install /usr/bin/ar ar /usr/bin/aarch64-bec-linux-ar 100
	update-alternatives --install /usr/bin/as as /usr/bin/aarch64-bec-linux-as 100
	update-alternatives --install /usr/bin/c++filt c++filt /usr/bin/aarch64-bec-linux-c++filt 100
	update-alternatives --install /usr/bin/elfedit elfedit /usr/bin/aarch64-bec-linux-elfedit 100
	update-alternatives --install /usr/bin/gprof gprof /usr/bin/aarch64-bec-linux-gprof 100
	update-alternatives --install /usr/bin/ld ld /usr/bin/aarch64-bec-linux-ld 100
	update-alternatives --install /usr/bin/ld.bfd ld.bfd /usr/bin/aarch64-bec-linux-ld.bfd 100
	update-alternatives --install /usr/bin/ld.gold ld.gold /usr/bin/aarch64-bec-linux-ld.gold 100
	update-alternatives --install /usr/bin/dwp dwp /usr/bin/aarch64-bec-linux-dwp 100
	update-alternatives --install /usr/bin/nm nm /usr/bin/aarch64-bec-linux-nm 100
	update-alternatives --install /usr/bin/objcopy objcopy /usr/bin/aarch64-bec-linux-objcopy 100
	update-alternatives --install /usr/bin/objdump objdump /usr/bin/aarch64-bec-linux-objdump 100
	update-alternatives --install /usr/bin/ranlib ranlib /usr/bin/aarch64-bec-linux-ranlib 100
	update-alternatives --install /usr/bin/readelf readelf /usr/bin/aarch64-bec-linux-readelf 100
	update-alternatives --install /usr/bin/size size /usr/bin/aarch64-bec-linux-size 100
	update-alternatives --install /usr/bin/strings strings /usr/bin/aarch64-bec-linux-strings 100
	update-alternatives --install /usr/bin/strip strip /usr/bin/aarch64-bec-linux-strip 100
if [ x"$D" = "x" ]; then
	if [ -x /sbin/ldconfig ]; then /sbin/ldconfig ; fi
fi

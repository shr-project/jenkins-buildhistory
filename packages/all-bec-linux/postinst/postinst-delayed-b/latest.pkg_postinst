#!/bin/sh
set -e

if [ -n "$D" ]; then
    $INTERCEPT_DIR/postinst_intercept delay_to_first_boot postinst-delayed-b mlprefix=
    exit 0
fi
    set -e

    if [ ! -e /etc/postinst-test/delayed-a ]; then
        echo "postinst-delayed-b: /etc/postinst-test/delayed-a not found"
        exit 1
    fi

    touch /etc/postinst-test/delayed-b

	cd /boot
	for DTB in ${KERNEL_DEVICETREE}; do
		for type in zImage; do
			symlink_name=${type}"-"qemuarm
			DTB_BASE_NAME=`basename ${DTB} | awk -F "." '{print $1}'`
			DTB_SYMLINK_NAME=`echo ${symlink_name} | sed "s/qemuarm/${DTB_BASE_NAME}/g"`
			update-alternatives --install /boot/${DTB_BASE_NAME}.dtb ${DTB_BASE_NAME}.dtb /boot/devicetree-${DTB_SYMLINK_NAME}.dtb 40411 || true
		done
	done

	cd /boot
	for DTB in ${KERNEL_DEVICETREE}; do
		for type in bzImage; do
			symlink_name=${type}"-"qemux86-64
			DTB_BASE_NAME=`basename ${DTB} | awk -F "." '{print $1}'`
			DTB_SYMLINK_NAME=`echo ${symlink_name} | sed "s/qemux86-64/${DTB_BASE_NAME}/g"`
			update-alternatives --remove ${DTB_BASE_NAME}.dtb /boot/devicetree-${DTB_SYMLINK_NAME}.dtb 40411 || true
		done
	done

set -e\n    if [ -n "$D" ]; then\n        for I in $D/usr/lib/lib*.chk; do\n            DN=`dirname $I`\n            BN=`basename $I .chk`\n            FN=$DN/$BN.so\n            shlibsign -i $FN\n            if [ $? -ne 0 ]; then\n                exit 1\n            fi\n        done\n    else\n        signlibs.sh\n    fi\n